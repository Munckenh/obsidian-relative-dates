{
	"name": "obsidian-relative-dates",
	"version": "1.0.2",
	"description": "Relative, color-coded dates for Obsidian",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json package.json package-lock.json && git commit -m \"chore: $npm_package_version\" && git tag $npm_package_version -m \"\"",
		"lint": "eslint src/**/*.ts",
		"lint:fix": "eslint src/**/*.ts --fix"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"date",
		"relative"
	],
	"author": "Munckenh",
	"license": "MIT",
	"devDependencies": {
		"@codemirror/language": "^6.0.0",
		"@codemirror/state": "^6.0.0",
		"@codemirror/view": "^6.0.0",
		"@types/node": "^24.0.0",
		"@typescript-eslint/parser": "^8.0.0",
		"@typescript-eslint/eslint-plugin": "^8.0.0",
		"builtin-modules": "^3.3.0",
		"esbuild": "^0.25.8",
		"eslint": "^9.0.0",
		"eslint-plugin-obsidianmd": "^0.1.7",
		"obsidian": "latest",
		"tslib": "^2.4.0",
		"typescript": "^5.9.0"
	}
}
